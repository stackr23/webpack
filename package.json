{
  "name": "@viewar/webpack",
  "version": "1.0.0",
  "description": "shared webpack config of ViewAR",
  "main": "index.js",
  "env": {
    "WEBPACK_PATH": "test"
  },
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "start": "webpack-dev-server --config ./test/webpack.config.js --open --mode development --env development_mock",
    "build": "webpack -p --config ./test/webpack.config.js --mode production --env production",
    "test": "npm run start",
    "lint": "eslint . --format pretty --fix"
  },
  "repository": {
    "type": "git",
    "url": "dev@it5.at:/var/git/repositories/packages/webpack"
  },
  "keywords": [
    "viewar",
    "webpack",
    "config",
    "shared"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "esmodules": true,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix --format pretty"
    ]
  },
  "author": "Stefan Friedl <sf@viewar.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "dotenv": "^8.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-port-check": "^2.0.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass": "^1.23.0-module.beta.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "viewar-core": "^11.41.1",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-viewar": "git+https://bitbucket.org/viewar_sf/eslint-config-viewar.git",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint_d": "^8.0.0",
    "express-useragent": "^1.0.13",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {}
}
