{
  "name": "@stackr23/webpack",
  "version": "2.0.0",
  "description": "shared webpack config of StackR23",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "start": "cross-env WEBPACK_PATH=test webpack-dev-server --open --mode development --env development_core",
    "build": "npm run clean && cross-env WEBPACK_PATH=test NODE_ENV=production webpack -p --mode production --env production",
    "watch": "cross-env WEBPACK_PATH=test NODE_ENV=production webpack --watch -p --mode production --env production",
    "test": "npm run lint && npm run build && npm run karma",
    "karma": "cross-env WEBPACK_PATH=test karma start ./src/test/karma.config.js",
    "lint": "cross-env WEBPACK_PATH=test eslint --fix \"**/*.*{js,jsx}\" --ignore-pattern \"build/**/*\"",
    "format": "prettier **/*.{css,scss,md,json} --write"
  },
  "homepage": "https://github.com/stackr23",
  "repository": {
    "type": "git",
    "url": "https://github.com/stackr23/webpack"
  },
  "bugs": {
    "url": "https://github.com/stackr23/webpack/issues"
  },
  "files": [
    "src/*",
    "babel.config.js",
    "karma.js",
    "middlewares.js",
    "remoteConsole.js"
  ],
  "private": false,
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "keywords": [
    "StackR23",
    "webpack",
    "shared",
    "config"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss,json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix --quiet"
    ]
  },
  "author": "DoubleU23 <5007050+DoubleU23@users.noreply.github.com>",
  "license": "MIT",
